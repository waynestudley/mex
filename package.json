{
  "name": "react-app",
  "version": "0.1.0",
  "homepage": "./",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "cordova-android": "^8.1.0",
    "formik": "^2.1.4",
    "i18next": "^19.4.4",
    "i18next-browser-languagedetector": "^4.1.1",
    "i18next-http-backend": "^1.0.8",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.4.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-signature-canvas": "^1.0.3"
  },
  "scripts": {
    "start": "npm run remove:www-mac && react-scripts start",
    "build": "npm run remove:www-mac && react-scripts build",
    "start:pc": "npm run remove:www-pc && react-scripts start",
    "build:pc": "npm run remove:www-pc && react-scripts build",
    "build:cordova": "npm run remove:www-pc && react-scripts build && node __script.js && mv build www && cordova build",
    "test": "react-scripts test",
    "coverage": "react-scripts test --env=jsdom --watchAll=false --coverage",
    "eject": "npm run remove:www-pc && react-scripts eject",
    "remove:www-pc": "if exist www rd /s /q www",
    "remove:www-mac": "rm -rf www"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "axios": "^0.19.2",
    "cordova-plugin-whitelist": "^1.3.4",
    "node-sass": "^4.14.1"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {}
    },
    "platforms": [
      "android"
    ]
  }
}
